---
import Analytics from "@components/page/Analytics.astro";
import Basehead from "@components/page/Basehead.astro";
import Footer from "@components/page/Footer.astro";
import Header from "@components/page/Header.astro";

const { metaData } = Astro.props;
---

<!doctype html>
<html lang="en">
  <Basehead metaData={metaData} />
  <body>
    <Header />
    <div class="content">
      <slot />
    </div>
    <Footer />
    <Analytics />
  </body>
</html>
